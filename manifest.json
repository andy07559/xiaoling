{
  "manifest_version": 3,
  "name": "小灵导航",
  "version": "2.1",
  "description": "一键生成08.ink短网址和导航",
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "storage",
    "cookies"
  ],
  "host_permissions": [
    "http://08.ink/*",
    "https://08.ink/*",
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
} 